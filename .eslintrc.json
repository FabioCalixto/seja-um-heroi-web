{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "airbnb-base",
      "prettier"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "globals": {
    "Atomics": "readonly",
    "ShareArrayBuffer": "readonly"
  },
  "plugins": [
      "react",
      "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
  "class-methods-use-this": "off",
  "no-param-reassign": "off",
  "camelcase": "off",
  "no-unused-vars": ["error", { "argsIgnorePattern": "next"}],
  "semi": "off"
  }
}
